{
  "version": 2,
  "builds": [
    {
      "src": "index.js",    // O arquivo de entrada do seu servidor
      "use": "@vercel/node" // Usando a build do Vercel para Node.js
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"  // Roteando todas as requisições para o servidor Node.js
    }
  ]
}